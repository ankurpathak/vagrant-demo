echo insecure >> ~/.curlrc

source "$SDKMAN_DIR/bin/sdkman-init.sh"

export SDKMAN_DIR="/usr/local/sdkman" && cat index.html | bash

wget --no-check-certificate  https://get.sdkman.io

//replace all curl with curl -k

export SDKMAN_DIR="/usr/local/sdkman" && curl -k "https://get.sdkman.io" | bash

apt install zip unzip



npm config set registry http://registry.npmjs.org/


npm config set cafile "<path to certificate file>"

npm config set strict-ssl false
